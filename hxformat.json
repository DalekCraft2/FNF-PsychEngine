{
	"$schema": "https://raw.githubusercontent.com/HaxeCheckstyle/haxe-formatter/master/resources/hxformat-schema.json",
	"lineEnds": {
		"leftCurly": "both",
		"rightCurly": "both",
		"emptyCurly": "break",
		"objectLiteralCurly": {
			"leftCurly": "after"
		}
	},
	"sameLine": {
		"ifElse": "next",
		"doWhile": "next",
		"tryBody": "next",
		"tryCatch": "next"
	}
}
